    <!-- java order summary text -->
<string name="order_summary">"Résumé de votre commande : "</string>
    <!--
        name for the order summary. it will be shown in the format of 'Name: Anne"
        where Anne is the user's name. [CHAR LIMIT=none ]
    -->
<string name="order_summary_name">Nom : <xliff:g example="Anne" id="name_order">%s</xliff:g></string>
    <!--
      Whipped cream topping for the order summary. It will be shown in the format of
      "Avec de la crème ? true" or "Avec de la crème ? false". [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_whipped_cream">Avec de la crème ?\u0020 <xliff:g example="true" id="topping_whipped_cream">\%\b</xliff:g></string>
    <!--
    Chocolate topping for the order summary. It will be shown in the format of
    "Avec du chocolat ? true" or "Avec du chocolat ? false". [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_chocolate">Avec du chocolat ?\u0020  <xliff:g example="true" id="topping_chocolate">\%\b</xliff:g></string>
    <!--
    Quantity of coffee cups for the order summary. It will be shown in the format of
    "Quantité : 2", where 2 is the number of cups ordered. [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_quantity">Quantité :\u0020 <xliff:g example="2" id="quantity_order">\%\d</xliff:g></string>
    <!--
    Total price for the order summary. It will be shown in the format of
    "Total: 10€" where 10€ is the price. [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_price">Total:\u0020€ <xliff:g example="10€" id="price_order">\%\s</xliff:g></string>

<string name="order_summary_thank_you">Merci</string>


</resources>